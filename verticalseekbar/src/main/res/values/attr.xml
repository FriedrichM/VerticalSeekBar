<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="VerticalSeekBar">
        <attr name="vsb_drawable_corner_radius" format="dimension" />
        <attr name="vsb_drawable_width" format="dimension" />
        <attr name="vsb_drawable_background" format="reference|color" />
        <attr name="vsb_drawable_progress" format="reference|color" />
        <attr name="vsb_drawable_progress_gradient_start" format="color" />
        <attr name="vsb_drawable_progress_gradient_end" format="color" />
        <attr name="vsb_progress" format="integer" />
        <attr name="vsb_min_placeholder_src" format="reference" />
        <attr name="vsb_max_placeholder_src" format="reference" />
        <attr name="vsb_thumb_container_corner_radius" format="dimension" />
        <attr name="vsb_thumb_container_tint" format="color" />
        <attr name="vsb_thumb_placeholder_src" format="integer" />
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
    </declare-styleable>

</resources>
